    "@compodoc/compodoc": "^1.1.26",
    "@storybook/addon-docs": "^9.0.13",
    "@storybook/angular": "^9.0.13",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-plugin-storybook": "^9.0.13",
    "jasmine-core": "~5.7.0",
